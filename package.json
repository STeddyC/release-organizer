    "dotenv": "^16.4.5",
    "firebase": "^10.8.0",
    "ftp-deploy": "^2.4.6",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2",
    "stripe": "^14.14.0",
    "tailwind-merge": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^20.11.28",

{
  "name": "music-release-organizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:admin": "tsc && vite build --config vite.admin.config.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "node scripts/clean.js",
    "deploy": "node deploy.js",
    "deploy:rules": "firebase deploy --only storage,database --project release-organize --non-interactive",
    "deploy:database": "firebase deploy --only database --project release-organize --non-interactive",
    "firebase:use": "firebase use release-organize",
    "firebase:emulators": "firebase emulators:start"
  },
  "dependencies": {
    "@stripe/stripe-js": "^2.4.0",
